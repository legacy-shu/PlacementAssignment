@model MusicLibrary.Models.ViewModel
<head>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
</head>

<h3>All Track List</h3>
<table class="table table-hover">
  
    <thead>
    <tr>
        <td>Artist</td>
        <td>Title</td>
        <td>Duration</td>
        <td>Issued Date</td>
        <td>Genre</td>
    </tr>
    </thead>
  
    <tbody>
    @foreach(var track in Model.AllTracks)
    {
        <tr>
            <td>@track.Artist</td>
            <td>@track.Title</td>
            <td>@track.Duration</td>
            <td>@track.IssuedDate</td>
            <td>@track.Genre</td>
        </tr>
    }
    </tbody>
</table>

<h3>Filltered Track List</h3>
<table class="table table-hover">
  
    <thead>
    <tr>
        <td>Artist</td>
        <td>Title</td>
        <td>Duration</td>
        <td>Issued Date</td>
        <td>Genre</td>
    </tr>
    </thead>
  
    <tbody>
    @foreach(var track in Model.FillteredTracks)
    {
        <tr>
            <td>@track.Artist</td>
            <td>@track.Title</td>
            <td>@track.Duration</td>
            <td>@track.IssuedDate</td>
            <td>@track.Genre</td>
        </tr>
    }
    </tbody>
</table>

<ul>
    <li class="page-scroll">
        <a href="?genre=Pop">Pop</a>
    </li>
    <li class="page-scroll">
        <a href="?genre=K-Pop">K-Pop</a>
    </li>
    <li class="page-scroll">
        <a href="?genre=Rock">Rock</a>
    </li>
    <li class="page-scroll">
        <a href="/">Default</a>
    </li>
</ul>